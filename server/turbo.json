{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "start": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": []
    }
  }
}
