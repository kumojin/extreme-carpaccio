    "@types/lodash": "4.17.4",
    "argon2": "0.40.3",
    "big.js": "6.2.1",
    "body-parser": "1.20.2",
    "colors": "1.4.0",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "http-status-codes": "2.3.0",
    "joi": "17.13.1",
    "lodash": "4.17.21",
    "sqlite": "5.1.1",
    "sqlite3": "5.1.7",
    "winston": "3.13.0"
  },
  "devDependencies": {
    "@swc/core": "1.5.25",
